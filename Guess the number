  
secret_number = "7"
guess = ""
guess_limit = 3
guess_count = 0
out_of_guess = False

while guess != secret_number and not(out_of_guess):
    if guess_count < guess_limit:
        guess = input("Enter a number: ")
        guess_count += 1
    else:
        out_of_guess = True

if out_of_guess:
    print("You Lose!")
else:
    print("You Win")

